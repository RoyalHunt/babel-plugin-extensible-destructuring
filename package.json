{
  "name": "babel-plugin-extensible-destructuring",
  "version": "2.0.0",
  "description": "Babel plugin that enables extensible destructuring as per https://github.com/vacuumlabs/es-proposals",
  "author": "Markéta Lisová <themarketka@gmail.com>",
  "repository": "vacuumlabs/babel-plugin-extensible-destructuring",
  "keywords": [
    "babel-plugin",
    "destructuring"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel src --out-dir lib --copy-files",
    "watch": "npm run build -- --watch",
    "test": "mocha --compilers js:babel-core/register test/**",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "babel-register": "^6.2.0",
    "babel-runtime": "^6.2.0",
    "immutable": "^3.7.5"
  },
  "devDependencies": {
    "babel-cli": ">=6.2.0",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-plugin-external-helpers-2": "^6.1.18",
    "mocha": "^2.3.4"
  }
}
